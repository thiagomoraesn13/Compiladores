program Test {

	int i;
  
	// calcula soma de 1..i
	void SumUp() {
		int sum;
		sum = 0;
		while (i > 0) { sum = sum + i; i = i - 1; }
		write sum;
	}
    
	// programa comeca aqui!
	void Main() {
		read i;
		while (i > 0) {
			SumUp();
			read i;
		}
        if (i == 0) write 0; else write -1;
	}
}


